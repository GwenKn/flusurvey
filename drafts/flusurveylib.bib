@Article{Ashiru-Oredope2013,
  author          = {Ashiru-Oredope, Diane and Hopkins, Susan and English Surveillance Programme for Antimicrobial Utilization and Resistance Oversight Group},
  title           = {Antimicrobial stewardship: English Surveillance Programme for Antimicrobial Utilization and Resistance (ESPAUR).},
  journal         = {The Journal of antimicrobial chemotherapy},
  year            = {2013},
  volume          = {68},
  pages           = {2421--2423},
  month           = nov,
  issn            = {1460-2091},
  abstract        = {The clinical, public health and economic implications of antimicrobial resistance present a major threat to future healthcare. Antimicrobial use is a major driver of resistance, and antimicrobial stewardship programmes are increasingly being advocated as a means of improving the quality of prescribing. However, to increase their impact and assess their success, a better understanding of antimicrobial usage, both in primary and secondary care, and linkage with antimicrobial resistance data are required. In England, national summaries of primary care dispensing data are issued annually by the Health and Social Care Information Centre. However, there is currently no routine public reporting of antimicrobial usage in hospitals. In response to the threat posed by antimicrobial resistance, as highlighted in the Report of the Chief Medical Officer and on the request of the Department of Health, Public Health England has developed a new national programme, the English Surveillance Programme for Antimicrobial Utilization and Resistance (ESPAUR). The programme will bring together the elements of antimicrobial utilization and resistance surveillance in both primary and secondary care settings, alongside the development of quality measures and methods to monitor unintended outcomes of antimicrobial stewardship and both public and professional behaviour interventions. This article reports on the background to the programme development, the current oversight group membership and the public reporting structure. },
  chemicals       = {Anti-Bacterial Agents},
  citation-subset = {IM},
  completed       = {2014-04-23},
  country         = {England},
  doi             = {10.1093/jac/dkt363},
