  issue           = {4},
  keywords        = {Adolescent; Adult; Age Factors; Aged; Anti-Bacterial Agents, therapeutic use; Data Collection; Education; England; Ethnic Groups; Female; Health Education; Health Knowledge, Attitudes, Practice; Health Promotion; Humans; Male; Middle Aged; Scotland; Sex Factors; Socioeconomic Factors; Surveys and Questionnaires; Wales},
  nlm-id          = {7513617},
